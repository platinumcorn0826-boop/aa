{
    "version": 2,
    "name": "memento-countdown",
    "cleanUrls": true,
    "framework": null,
    "headers": [
        {
            "source": "/(js/sw.js|manifest.json)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Service-Worker-Allowed",
                    "value": "/"
                }
            ]
        }
    ]
}